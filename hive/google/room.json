{
  "name": "google/room",
  "description": "Room provides an abstraction layer over SQLite to allow fluent database access while harnessing the full power of SQLite.",
  "homepage": "https://developer.android.com/topic/libraries/architecture/room.html",
  "dependencies": [
    {
      "dependency": "android.arch.persistence.room:runtime",
      "compileType": "implementation"
    },
    {
      "dependency": "android.arch.persistence.room:rxjava2",
      "compileType": "implementation"
    },
    {
      "dependency": "android.arch.persistence.room:compiler",
      "compileType": "annotationProcessor"
    },
    {
      "dependency": "android.arch.persistence.room:testing",
      "compileType": "androidTestImplementation"
    }
  ],
  "repository": {
    "server": "maven",
    "url": "https://maven.google.com"
  }
}
